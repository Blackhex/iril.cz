{
  "name": "iril.cz",
  "version": "1.0.0",
  "private": true,
  "description": "IRIL - Reiki v našem životě a reiki kurzy v regionech Brno a Tábor",
  "main": "index.html",
  "dependencies": {
    "bootstrap": ">=4.1.3",
    "cryptiles": ">=4.1.2",
    "daemonite-material": ">=4.1.1",
    "jquery": ">=3.3.1",
    "hoek": ">=4.2.1",
    "mdbootstrap": "^4.5.15",
    "openlayers": ">=4.6.5",
    "pickadate": ">=3.5.6",
    "popper.js": "^1.14.6",
    "tether": ">=1.4.5"
  },
  "devDependencies": {
    "connect": ">=3.6.6",
    "cookie-signature": ">=1.1.0",
    "debug": ">=4.1.0",
    "fresh": ">=0.5.2",
    "less": "^3.9.0",
    "mime": "^2.4.0",
    "negotiator": ">=0.6.1",
    "qs": "^6.6.0",
    "send": ">=0.16.2",
    "simple-autoreload-server": ">=0.2.7"
  },
  "scripts": {
    "build_landing": "lessc less/landing.less css/landing.css",
    "build_reiki": "lessc less/reiki.less css/reiki.css",
    "build_angels": "lessc less/angels.less css/angels.css",
    "build_tarot": "lessc less/tarot.less css/tarot.css",
    "build_massages": "lessc less/massages.less css/massages.css",
    "build": "npm run build_landing && npm run build_reiki && npm run build_angels && npm run build_tarot && npm run build_massages",
    "start": "autoreload-server . 8080"
